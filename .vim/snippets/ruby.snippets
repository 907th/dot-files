snippet #!
	#!/usr/bin/env ruby
snippet #enc
	# encoding: utf-8
snippet des
	describe ${1:object} do
		${2}
	end
snippet it
	it ${1:acts} do
		${2}
	end
snippet ctx
	context ${1:description} do
		${2}
	end
snippet do
	do
		${1}
	end
snippet dov
	do |${1:var}|
		${2}
	end
snippet exp
	expect(${1}).to ${2}
snippet nexp
	expect(${1}).not_to ${2}
